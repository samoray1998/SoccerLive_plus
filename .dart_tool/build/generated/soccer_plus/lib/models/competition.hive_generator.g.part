class CompetitionAdapter extends TypeAdapter<Competition> {
  @override
  final int typeId = 1;

  @override
  Competition read(BinaryReader reader) {
    final numOfFields = reader.readByte();
    final fields = <int, dynamic>{
      for (int i = 0; i < numOfFields; i++) reader.readByte(): reader.read(),
    };
    return Competition(
      id: fields[0] as int,
      name: fields[1] as String,
      country: fields[2] as String,
      logo: fields[3] as String,
      flag: fields[4] as String,
      currentSeason: fields[5] as int?,
      isPopular: fields[6] as bool,
    );
  }

  @override
  void write(BinaryWriter writer, Competition obj) {
    writer
      ..writeByte(7)
      ..writeByte(0)
      ..write(obj.id)
      ..writeByte(1)
      ..write(obj.name)
      ..writeByte(2)
      ..write(obj.country)
      ..writeByte(3)
      ..write(obj.logo)
      ..writeByte(4)
      ..write(obj.flag)
      ..writeByte(5)
      ..write(obj.currentSeason)
      ..writeByte(6)
      ..write(obj.isPopular);
  }

  @override
  int get hashCode => typeId.hashCode;

  @override
  bool operator ==(Object other) =>
      identical(this, other) ||
      other is CompetitionAdapter &&
          runtimeType == other.runtimeType &&
          typeId == other.typeId;
}
